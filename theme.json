{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"settings": {
		"color": {
			"gradients": [
				{
					"gradient": "linear-gradient(133deg, var(--wp--preset--color--nitz), var(--wp--preset--color--secondary))",
					"name": "Nitz til secondary",
					"slug": "gradient-nr-1"
				}
			],
			"palette": [
				{
					"color": "#fff",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#085916",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#f4dd77",
					"name": "Nitz",
					"slug": "nitz"
				}
			]
		},
		"layout": {
			"contentSize": "450px",
			"wideSize": "1100px"
		},
		"spacing": {
			"margin": true,
			"padding": true,
			"units": [
				"px",
				"em",
				"rem",
				"vh",
				"vw",
				"%",
				"ch"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"name": "System Fonts",
					"slug": "system-fonts"
				},
				{
					"fontFamily": "Geneva, Tahoma, Verdana, sans-serif",
					"slug": "geneva-verdana"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Hoka",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": "file:./assets/fonts/hoka-red/HokaRed.otf"
						}
					],
					"fontFamily": "Hoka",
					"name": "Hoka",
					"slug": "hoka",
					"src": "file:./assets/fonts/hoka-red/HokaRed.otf"
				}
			],
			"fontSizes": [
				{
					"name": "Extra small",
					"size": "1rem",
					"slug": "extra-small"
				},
				{
					"name": "Small",
					"size": "1.125rem",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "1.25rem",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "1.5rem",
					"slug": "large"
				},
				{
					"name": "Extra large",
					"size": "2.5rem",
					"slug": "extra-large"
				}
			],
			"lineHeight": true
		}
	},
	"styles": {
		"blocks": {
			"core/heading": {
				"typography": {
					"fontFamily": "var:preset|font-family|hoka",
					"fontStyle": "normal",
					"fontWeight": "300"
				}
			},
			"core/list": {
				"typography": {
					"fontFamily": "var:preset|font-family|geneva-verdana"
				}
			},
			"core/post-featured-image": {
				"spacing": {
					"margin": {
						"bottom": "var:preset|spacing|30",
						"left": "var:preset|spacing|30",
						"right": "var:preset|spacing|30",
						"top": "var:preset|spacing|30"
					},
					"padding": {
						"bottom": "0",
						"left": "0",
						"right": "0",
						"top": "0"
					}
				}
			},
			"core/site-logo": {
				"spacing": {
					"margin": {
						"bottom": "var:preset|spacing|20",
						"left": "var:preset|spacing|20",
						"right": "var:preset|spacing|20",
						"top": "var:preset|spacing|20"
					}
				}
			},
			"core/site-tagline": {
				"typography": {
					"fontSize": "var:preset|font-size|extra-small"
				}
			},
			"core/site-title": {
				"color": {
					"text": "var:preset|color|light-green-cyan"
				},
				"spacing": {
					"margin": {
						"bottom": "var:preset|spacing|20",
						"left": "var:preset|spacing|20",
						"right": "var:preset|spacing|20",
						"top": "var:preset|spacing|20"
					},
					"padding": {
						"bottom": "0",
						"left": "0",
						"right": "0",
						"top": "0"
					}
				},
				"typography": {
					"fontSize": "var:preset|font-size|extra-large",
					"fontStyle": "normal",
					"fontWeight": "100",
					"textTransform": "capitalize"
				}
			}
		},
		"color": {
			"background": "var:preset|color|nitz",
			"text": "var:preset|color|black"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "Hoka",
					"fontSize": "3.3rem"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "Hoka"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "Hoka"
				}
			},
			"link": {
				"typography": {
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"fontStyle": "normal",
					"fontWeight": "700"
				}
			}
		},
		"spacing": {
			"padding": {
				"bottom": "0",
				"left": "0",
				"right": "0",
				"top": "0"
			}
		},
		"typography": {
			"fontFamily": "var:preset|font-family|geneva-verdana",
			"fontSize": "var:preset|font-size|large",
			"fontStyle": "normal",
			"fontWeight": "400"
		}
	},
	"version": 2
}